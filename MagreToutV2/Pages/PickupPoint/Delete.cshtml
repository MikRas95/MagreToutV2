@page
@model MalgreToutV2.Pages.PickupPoint.DeleteModel
@{
    ViewData["Title"] = "Delete";
}
<h1>Slet Opsamlingssted</h1>
<h3>Er du sikker på du vil slette denne?</h3>
<div>
    <form method="post">
        <dl class="row">

            <dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.PickupPointId)   </dt>
            <dd class="col-sm-10">  @Model.PickupPoint.PickupPointId  </dd>
            <dt class="col-sm-2"> @Html.DisplayNameFor(model => model.PickupPoint.Name)  </dt>
            <dd class="col-sm-10">  @Model.PickupPoint.Name  </dd>
            <dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.Address)  </dt>
            <dd class="col-sm-10"> @Model.PickupPoint.Address  </dd>
            <dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.Country) </dt>
            <dd class="col-sm-10">  @Model.PickupPoint.Country </dd>
            <dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.PostalCode)  </dt>
            <dd class="col-sm-10"> @Model.PickupPoint.PostalCode  </dd>
            <dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.NoOfMagazine) </dt>
            <dd class="col-sm-10">  @Model.PickupPoint.NoOfMagazine </dd>
            @*<dt class="col-sm-2">  @Html.DisplayNameFor(model => model.PickupPoint.ContactPersonId) </dt>
            <dd class="col-sm-10">  @Model.PickupPoint.ContactPersonId </dd>*@
        </dl>
        <input type="hidden" asp-for="PickupPoint.PickupPointId" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <br/>
        <br/>
        <div>
            <a class="btn btn-outline-secondary" asp-page="/PickupPoint/Read">Tilbage</a>
        </div>
    </form>
</div>